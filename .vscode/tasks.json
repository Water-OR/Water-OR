{
  "version": "2.0.0",
  "tasks": [
    {
      "type": "shell",
      "label": "WOR-c++",
      "command": "g++",
      "args": [
        "-g",
        "${file}",
        "-o",
        "${fileDirname}\\${fileBasenameNoExtension}.exe",
        "-std=c++17",
        "-Wall",
        "-static-libgcc"
      ],
      "options": {
        "cwd": "D:\\MinGW\\bin"
      },
      "problemMatcher": [
        "$gcc"
      ]
    },
    {
      "type": "shell",
      "label": "WOR-c",
      "command": "gcc",
      "args": [
        "-g",
        "${file}",
        "-o",
        "${fileDirname}\\${fileBasenameNoExtension}.exe",
        "-std=c11",
        "-Wall",
        "-static-libgcc"
      ],
      "options": {
        "cwd": "D:\\MinGW\\bin"
      },
      "problemMatcher": [
        "$gcc"
      ]
    }
  ]
}